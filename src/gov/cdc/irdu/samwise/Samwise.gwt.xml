<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='samwise'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name='com.google.gwt.xml.XML'/>

  <inherits name='com.google.gwt.user.theme.standard.Standard'/>
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

  <!-- Other module inherits                                      -->

  <!-- Specify the app entry point class.                         -->
  <entry-point class='gov.cdc.irdu.samwise.client.Samwise'/>
  
  <!-- Specify remote services
  <servlet path='/document' class='gov.cdc.irdu.samwise.server.DocumentServiceImpl' />
  <servlet path='/uploadFile' class='gov.cdc.irdu.samwise.server.UploadFileServlet' />
  -->
  
  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>

</module>
